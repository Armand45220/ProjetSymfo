<html lang="fr">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1." />
    <title>Ajout partenaire</title>
    <link href="{{ asset('static/css/app.css') }}" rel="stylesheet"/>
</head>
<body>
<!-- formulaire pour modifier un reglement ou un mail avec un hypertext de type balloon -->
<h1 class="title_bo">Modifier un propos</h1>
    <div id="part_form" style="margin-top: 50px;display: flex;flex-direction: column;align-items: center;">
        {{ form_start(form) }}

        <p>email:</p>
        {{ form_row(form.email) }}
        <div id="editorProposEmailModif" style="border: solid 1px"></div>
        <p>Description des r√®glements :</p>
        {{ form_row(form.descReglements) }}
        <div id="editorProposDescRegleModif" style="border: solid 1px"></div>


        {{ form_end(form) }}
    </div>
    <div class="back_path">
        <a href= {{ path('aProposAffiche') }}>Retour</a>
    </div>
</body>
<!-- les lien pour le formulaire en hypertext -->
{% block javascripts %}
    <script src="{{ asset('ckeditor/ckeditor.js') }}"></script>
    <script src="{{ asset('js/ckeditor.js') }}"></script>
{% endblock %}
</html>